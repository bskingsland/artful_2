<div class="page-header">
  <h1>
    Visits
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
      </tr>

      <% @visits.each do |visit| %>
      <tr>
        <td><%= visit.space.name %></td>

      </tr>
      <% end %>
    </table>
  </div>
</div>
